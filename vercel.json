{
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/services/web/",
      "destination": "/services/web"
    },
    {
      "source": "/services/ec/",
      "destination": "/services/ec"
    },
    {
      "source": "/services/video/",
      "destination": "/services/video"
    },
    {
      "source": "/services/furniture/",
      "destination": "/services/furniture"
    },
    {
      "source": "/legal/terms/",
      "destination": "/legal/terms"
    },
    {
      "source": "/legal/privacy-policy/",
      "destination": "/legal/privacy-policy"
    }
  ]
}